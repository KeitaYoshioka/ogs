<?xml version="1.0" encoding="ISO-8859-1"?>
<OpenGeoSysProject>
    <meshes>
        <mesh>insitu_pf.vtu</mesh>
        <mesh>tut_bottom.vtu</mesh>
        <mesh>tut_top.vtu</mesh>
        <mesh>tut_left.vtu</mesh>
        <mesh>tut_right.vtu</mesh>
        <mesh>tut_p_0.vtu</mesh>
        <mesh>tut_p_1.vtu</mesh>
    </meshes>
    <processes>
        <process>
            <name>PhaseFieldInSitu</name>
            <type>PHASE_FIELD_INSITU</type>
            <hydro_crack_scheme>propagating</hydro_crack_scheme>
            <at_num>1</at_num>
            <split_method>3</split_method>
            <reg_param>1.e-5</reg_param>
            <integration_order>2</integration_order>
            <constitutive_relation>
                <type>LinearElasticIsotropic</type>
                <youngs_modulus>E</youngs_modulus>
                <poissons_ratio>nu</poissons_ratio>
            </constitutive_relation>
            <phasefield_parameters>
                <residual_stiffness>k</residual_stiffness>
                <crack_resistance>gc</crack_resistance>
                <crack_length_scale>ls</crack_length_scale>
            </phasefield_parameters>
            <solid_density>rho_sr</solid_density>
            <process_variables>
                <phasefield>phasefield</phasefield>
                <displacement0>displacement0</displacement0>
                <displacement1>displacement1</displacement1>
            </process_variables>
            <secondary_variables>
                <secondary_variable type="static" internal_name="sigma" output_name="sigma"/>
                <secondary_variable type="static" internal_name="epsilon" output_name="epsilon"/>
            </secondary_variables>
            <specific_body_force>0 0 </specific_body_force>
        </process>
    </processes>
    <time_loop>
        <global_process_coupling>
            <max_iter> 1000 </max_iter>
            <convergence_criteria>
                <!-- convergence criterion for the first process -->
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <abstol>1.e-1</abstol>
                    <reltol>1.e-1</reltol>
                </convergence_criterion>
                <!-- convergence criterion for the second process -->
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <abstol>1.e-1</abstol>
                    <reltol>1.e-1</reltol>
                </convergence_criterion>
                <!-- convergence criterion for the third process -->
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>INFINITY_N</norm_type>
                    <abstol>1.e-4</abstol>
                    <reltol>1.e-10</reltol>
                </convergence_criterion>
            </convergence_criteria>
        </global_process_coupling>
        <processes>
            <process ref="PhaseFieldInSitu">
                <nonlinear_solver>basic_newton_u</nonlinear_solver>
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <reltol>1.e-8</reltol>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial>0</t_initial>
                    <t_end>0.001</t_end>
                    <timesteps>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>0.001</delta_t>
                        </pair>
                    </timesteps>
                </time_stepping>
            </process>
            <process ref="PhaseFieldInSitu">
                <nonlinear_solver>basic_newton_u</nonlinear_solver>
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <reltol>1.e-8</reltol>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial>0</t_initial>
                    <t_end>0.001</t_end>
                    <timesteps>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>0.001</delta_t>
                        </pair>
                    </timesteps>
                </time_stepping>
            </process>
            <process ref="PhaseFieldInSitu">
                <nonlinear_solver>petsc_snes</nonlinear_solver>
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <reltol>1.e-14</reltol>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial>0</t_initial>
                    <t_end>0.001</t_end>
                    <timesteps>
                        <pair>
                            <repeat>100</repeat>
                            <delta_t>0.001</delta_t>
                        </pair>
                    </timesteps>
                </time_stepping>
            </process>
        </processes>
        <output>
            <variables>
                <variable>displacement0</variable>
                <variable>phasefield</variable>
                <variable>displacement1</variable>
                <variable>sigma</variable>
                <variable>epsilon</variable>
            </variables>
            <type>VTK</type>
            <prefix>GEMexRuns_SensAn_01Gc001</prefix>
            <timesteps>
                <pair>
                    <repeat>10</repeat>
                    <each_steps>1</each_steps>
                </pair>
                <pair>
                    <repeat>10</repeat>
                    <each_steps>1</each_steps>
                </pair>
                <pair>
                    <repeat>10</repeat>
                    <each_steps>1</each_steps>
                </pair>
            </timesteps>
        </output>
    </time_loop>
    <parameters>
        <!-- Mechanics -->
        <parameter>
            <name>E</name>
            <type>Constant</type>
            <value>1.0</value>
        </parameter>
        <parameter>
            <name>nu</name>
            <type>Constant</type>
            <value>0.27</value>
<!--             <type>MeshElement</type>
            <field_name>nu</field_name> -->
        </parameter>
        <parameter>
            <name>k</name>
            <type>Constant</type>
            <value>1e-8</value>
        </parameter>
        <parameter>
            <name>gc</name>
            <type>Constant</type>
            <value>1.0</value>
<!--             <type>MeshElement</type>
            <field_name>Gc</field_name> -->
        </parameter>
        <parameter>
            <name>ls</name>
            <type>Constant</type>
            <value>0.15</value>
        </parameter>
        <parameter>
            <name>rho_sr</name>
            <type>Constant</type>
            <value>0.0</value>
        </parameter>
        <parameter>
            <name>displacement_zero</name>
            <type>Constant</type>
            <values>0 0</values>
        </parameter>
        <parameter>
            <name>phasefield_ic</name>
            <type>MeshNode</type>
            <field_name>pf-ic</field_name>
           <!--  <type>Constant</type>
            <value>1</value> -->
        </parameter>
        <parameter>
            <name>phasefield_bc</name>
            <type>Constant</type>
            <value>1</value>
        </parameter>
        <parameter>
            <name>dirichlet0</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>

        <parameter>
            <name>dirichlet_right</name>
            <type>Constant</type>
            <value>-3.5</value>
        </parameter>
        <parameter>
            <name>dirichlet_left</name>
            <type>Constant</type>
            <value>3.5</value>
        </parameter>
        <parameter>
            <name>dirichlet_right_time</name>
            <type>CurveScaled</type>
            <curve>dirichlet_time</curve>
            <parameter>dirichlet_right</parameter>
        </parameter>
        <parameter>
            <name>dirichlet_left_time</name>
            <type>CurveScaled</type>
            <curve>dirichlet_time</curve>
            <parameter>dirichlet_left</parameter>
        </parameter>
        <parameter>
            <name>dirichlet_top_time</name>
            <type>CurveScaled</type>
            <curve>dirichlet_time</curve>
            <parameter>dirichlet_top</parameter>
        </parameter>
        <parameter>
            <name>dirichlet_bottom_time</name>
            <type>CurveScaled</type>
            <curve>dirichlet_time</curve>
            <parameter>dirichlet_bottom</parameter>
        </parameter>
        <parameter>
            <name>dirichlet_top</name>
            <type>Constant</type>
            <value>-1.0</value>
        </parameter>
        <parameter>
            <name>dirichlet_bottom</name>
            <type>Constant</type>
            <value>1.0</value>
        </parameter>

        <parameter>
            <name>force_top</name>
            <type>Constant</type>
            <value>0.</value>
        </parameter>
        <parameter>
            <name>force_bottom</name>
            <type>Constant</type>
            <value>0.</value>
        </parameter>
        <parameter>
            <name>force_left</name>
            <type>Constant</type>
            <value>3</value>
        </parameter>
        <parameter>
            <name>force_right</name>
            <type>Constant</type>
            <value>-3</value>
        </parameter>

        <parameter>
            <name>force_left_time</name>
            <type>CurveScaled</type>
            <curve>neumann_time</curve>
            <parameter>force_left</parameter>
        </parameter>
        <parameter>
            <name>force_right_time</name>
            <type>CurveScaled</type>
            <curve>neumann_time</curve>
            <parameter>force_right</parameter>
        </parameter>

    </parameters>
    <curves>
        <curve>
            <name>dirichlet_time</name>
            <coords>0 0.01 1.0</coords>
            <values>0 1.0  1.0</values>
        </curve>
        <curve>
            <name>neumann_time</name>
            <coords>0 0.005 1.0</coords>
            <values>0 1.0  1.0</values>
        </curve>
    </curves>

    <process_variables>

        <process_variable>
            <name>displacement0</name>
            <components>2</components>
            <order>1</order>
            <initial_condition>displacement_zero</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <mesh>tut_bottom</mesh>
                    <type>Dirichlet</type>
                    <component>1</component>
                    <parameter>dirichlet0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_right</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>dirichlet0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_top</mesh>
                    <type>Dirichlet</type>
                    <component>1</component>
                    <parameter>dirichlet0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_left</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>dirichlet0</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>

        <process_variable>
            <name>displacement1</name>
            <components>2</components>
            <order>1</order>
            <initial_condition>displacement_zero</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <mesh>tut_left</mesh>
                    <!-- <type>Dirichlet</type> -->
                    <type>Neumann</type>
                    <component>0</component>
                    <!-- <parameter>dirichlet0</parameter> -->
                    <parameter>force_left</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_right</mesh>
                    <!-- <type>Dirichlet</type> -->
                    <type>Neumann</type>
                    <component>0</component>
                    <!-- <parameter>dirichlet_right</parameter> -->
                    <parameter>force_right</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_bottom</mesh>
                    <!-- <type>Dirichlet</type> -->
                    <type>Neumann</type>
                    <component>1</component>
                    <!-- <parameter>dirichlet0</parameter> -->
                    <parameter>force_bottom</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_top</mesh>
                    <!-- <type>Dirichlet</type> -->
                    <type>Neumann</type>
                    <component>1</component>
                    <!-- <parameter>dirichlet_bottom</parameter> -->
                    <parameter>force_top</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_p_0</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>dirichlet0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_p_0</mesh>
                    <type>Dirichlet</type>
                    <component>1</component>
                    <parameter>dirichlet0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_p_1</mesh>
                    <type>Dirichlet</type>
                    <component>1</component>
                    <parameter>dirichlet0</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>

        <process_variable>
            <name>phasefield</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>phasefield_ic</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <mesh>tut_right</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>phasefield_bc</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_left</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>phasefield_bc</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_bottom</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>phasefield_bc</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_top</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>phasefield_bc</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>tut_right</mesh>
                    <type>PhaseFieldIrreversibleDamageOracleBoundaryCondition</type>
                    <component>0</component>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>petsc_snes</name>
            <type>PETScSNES</type>
            <max_iter>50</max_iter>
            <linear_solver>linear_solver_d</linear_solver>
        </nonlinear_solver>
        <nonlinear_solver>
            <name>basic_newton_u</name>
            <type>Newton</type>
            <max_iter>200</max_iter>
            <linear_solver>linear_solver_u</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>linear_solver_d</name>
            <eigen>
                <solver_type>BiCGSTAB</solver_type>
                <precon_type>ILUT</precon_type>
                <max_iteration_step>10000</max_iteration_step>
                <error_tolerance>1e-16</error_tolerance>
            </eigen>
            <petsc>
                <parameters>-ksp_type cg -pc_type bjacobi -ksp_atol 1e-16 -ksp_rtol 1e-16 -snes_type vinewtonssls -snes_linesearch_type l2 -snes_atol 1.e-10 -snes_rtol 1.e-10 -snes_max_it 1000 -snes_monitor </parameters>
            </petsc>
        </linear_solver>
        <linear_solver>
            <name>linear_solver_u</name>
            <!--eigen>
                <solver_type>BiCGSTAB</solver_type>
                <precon_type>ILUT</precon_type>
                <max_iteration_step>10000</max_iteration_step>
                <error_tolerance>1e-16</error_tolerance>
            </eigen>-->
            <petsc>
                <parameters>-ksp_type cg -pc_type hypre -pc_hypre_type boomeramg -pc_hypre_boomeramg_strong_threshold 0.7 -ksp_atol 1e-10 -ksp_rtol 1e-8 </parameters>
            </petsc>
        </linear_solver>
    </linear_solvers>
</OpenGeoSysProject>
